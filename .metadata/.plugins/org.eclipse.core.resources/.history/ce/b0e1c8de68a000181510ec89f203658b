package com.xoado.FilingCabinet.bean;

import java.util.Date;

public class TblBoxes {
    private String boxId;

    private String boxName;

    private String scopeofuse;

    private String moveable;

    private Object location;

    private Object ownerOrg;

    private Object shareto;

    private Object creater;

    private Date createrTime;

    private String remarks;

    private Integer useState;

    private String closeState;

    public String getBoxId() {
        return boxId;
    }

    public void setBoxId(String boxId) {
        this.boxId = boxId == null ? null : boxId.trim();
    }

    public String getBoxName() {
        return boxName;
    }

    public void setBoxName(String boxName) {
        this.boxName = boxName == null ? null : boxName.trim();
    }

    public String getScopeofuse() {
        return scopeofuse;
    }

    public void setScopeofuse(String scopeofuse) {
        this.scopeofuse = scopeofuse == null ? null : scopeofuse.trim();
    }

    public String getMoveable() {
        return moveable;
    }

    public void setMoveable(String moveable) {
        this.moveable = moveable == null ? null : moveable.trim();
    }

    public Object getLocation() {
        return location;
    }

    public void setLocation(Object location) {
        this.location = location;
    }

    public Object getOwnerOrg() {
        return ownerOrg;
    }

    public void setOwnerOrg(Object ownerOrg) {
        this.ownerOrg = ownerOrg;
    }

    public Object getShareto() {
        return shareto;
    }

    public void setShareto(Object shareto) {
        this.shareto = shareto;
    }

    public Object getCreater() {
        return creater;
    }

    public void setCreater(Object creater) {
        this.creater = creater;
    }

    public Date getCreaterTime() {
        return createrTime;
    }

    public void setCreaterTime(Date createrTime) {
        this.createrTime = createrTime;
    }

    public String getRemarks() {
        return remarks;
    }

    public void setRemarks(String remarks) {
        this.remarks = remarks == null ? null : remarks.trim();
    }

    public Integer getUseState() {
        return useState;
    }

    public void setUseState(Integer useState) {
        this.useState = useState;
    }

    public String getCloseState() {
        return closeState;
    }

    public void setCloseState(String closeState) {
        this.closeState = closeState == null ? null : closeState.trim();
    }

	@Override
	public String toString() {
		return "TblBoxes [boxId=" + boxId + ", boxName=" + boxName + ", scopeofuse=" + scopeofuse + ", moveable="
				+ moveable + ", location=" + location + ", ownerOrg=" + ownerOrg + ", shareto=" + shareto + ", creater="
				+ creater + ", createrTime=" + createrTime + ", remarks=" + remarks + ", useState=" + useState
				+ ", closeState=" + closeState + "]";
	}
//    初始创建
	public TblBoxes insertBoxes(String box_id,String boxname,String scopeofuse,String moveable,Object location,Object ownerOrg,Object shareto,Object creater,String remarks, Integer useState){
		TblBoxes boxes = new TblBoxes();
		boxes.setBoxId(box_id);
		boxes.setBoxName(boxname);
		boxes.setScopeofuse(scopeofuse);
		boxes.setMoveable(moveable);
		boxes.setLocation(location.toString());
		boxes.setOwnerOrg(ownerOrg.toString());
		boxes.setShareto(shareto.toString());
		boxes.setCreater(creater.toString());
		boxes.setCreaterTime(new Date());
		boxes.setRemarks(remarks);
		boxes.setUseState(useState);
		boxes.setCloseState("open");   // enum  close open   默认打开open 
		return boxes;
		
	}
}