 <select id="selectTheCurrentUser" parameterType="String" resultType="java.util.LinkedHashMap">
 SELECT a.archive_id,a.archive_name,a.owner_org,a.owner_user,a.comments,aattachment_picture,a.attachment_document,b.holddate,b.holdfrom FROM tbl_archives AS a,(SELECT * FROM tbl_current_archives AS c WHERE c.ownerid=&quot;${userid}&quot;) AS b WHERE a.archive_id=b.archive_id
 </select>
  
